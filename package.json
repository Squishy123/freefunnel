    "postcss-cli": "^7.1.1",
    "tailwindcss": "^1.7.3"
  },
  "dependencies": {
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.9.0",
    "postal-codes-js": "^2.5.1",
    "react-hook-form": "^6.5.1",
    "react-toast-notifications": "^2.4.0",
    "react-transition-group": "^4.4.1",
    "react-use-cart": "^1.9.0",
    "uuid": "^8.3.0"
