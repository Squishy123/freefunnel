    "start": "npm run watch:css && wp-scripts start",
    "test:e2e": "wp-scripts test-e2e",
    "test:unit": "wp-scripts test-unit-js",
    "watch": "chokidar \"**/*.scss\" -c \"npm run build\" --initial",
